<div class="row event_chat_main_block">
  <div class="w500_centerd">
    <div class="row front_page_title_row hidden_in_modal">
      <div class="col-12">
        <h3>User #<%= @front_side_current_user.id %> public page</h3>
      </div>
    </div>
    <div class="row front_page_content_row">
      <div class="col-12">
        <div class="row">
          <div class="col-12">
            <img src="<%= get_user_avatar_sm_url(@front_side_current_user) %>" />
          </div>
        </div>
        <div class="row">
          <div class="col-12">
            <span class='user_public_name'>Public user info: <%= @front_side_current_user.name_or_email_for_public.ehtml %></span>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
