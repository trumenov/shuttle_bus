<%= semantic_form_for [:admin, resource], url: addimage_admin_event_path(resource), :html => { :enctype => "multipart/form-data" } do |f| %>
  <%= f.semantic_errors(*f.object.errors.keys) %>
  <%= f.inputs do %>
    <%= f.inputs { f.input :img_file, as: :file } %>
  <% end %>
  <%= f.actions %>
<% end %>
