<div class="catalog_item_main_block" >
   <div class="catalog_item2">
      <a class="catalog_item_img_block2" href='<%= profile_my_trip_path(item.id) %>' onclick="return show_in_modal_link(this, 'proj_modal_main client_my_trip_modal');" name='my_trip_<%= item.id %>' >
        <% if item.user_route.valid_points_imgs.any? %>
          <div class="owl-carousel owl-theme owl_prod_list_slider">
            <% item.user_route.valid_points_imgs.each do |sm_img| %>
              <div class='item'>
                 <table>
                    <tbody>
                       <tr>
                          <td>
                            <img class='slider_sm_img' src='<%= sm_img.image_sm_url %>' />
                          </td>
                       </tr>
                    </tbody>
                 </table>
              </div>
            <% end %>
         </div>
        <% else %>
          <div class="catalog_item_subimg_block">
            <div class="catalog_item_img_cell">
              <img src="/pics/no_image.png" alt="" title="" />
            </div>
          </div>
        <% end %>
      </a>
      <div class="catalog_item_body">
         <div class="catalog_item_top_block">
            <div class="name_rate">
               <div class="catalog_item_title">
                  <a href="<%= profile_my_trip_path(item.id) %>" onclick="return show_in_modal_link(this, 'proj_modal_main client_my_trip_modal');" name='my_trip_<%= item.id %>'>
                    <%= item.user_route.name.to_s.ehtml %>
                  </a><br/><br/>
               </div>
            </div>
            <div class="row no-gutters">
              <div class="col-12">
                <%= item.publish_status_html %>
              </div>
            </div>
            <div class="row no-gutters">
              <div class="col-4">
                <label>Starts at:</label>
              </div>
              <div class="col-8">
                <%= item.trip_start_time.client_stamp_html %><br/><br/>
              </div>
            </div>
         </div>
      </div>
   </div>
</div>


